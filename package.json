{
  "name": "latextohtml",
  "version": "1.0.0",
  "description": "A latex to HTML conversion application based on electronJS",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . LaTeXToHTML --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"LaTeXToHTML\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/19khowes/LatexToHTMLApp.git"
  },
  "keywords": [
    "pandoc",
    "electron",
    "latex",
    "html"
  ],
  "author": "Kade Howes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/19khowes/LatexToHTMLApp/issues"
  },
  "homepage": "https://github.com/19khowes/LatexToHTMLApp#readme",
  "devDependencies": {
    "electron": "^13.1.7",
    "electron-packager": "^15.3.0"
  }
}
